        <name>guestWithReservation</name>
        <script><![CDATA[function guestWithReservation(){
	var result = [];
		var userList = new GlideRecord('x_94581_myhotel_m2m_uguests_ureservations');
	if(current.udate){
		userList.addQuery('ureservation.uarrival',current.udate);
	}
		userList.query();
		while(userList.next()){
			result.push(userList.getValue('uguests'));
		}
	gs.debug('DEBUG: guestWithReservation Array ' + result);
		var uniqList = new global.ArrayUtil();
		return uniqList.unique(result);
		
    }]]></script>
